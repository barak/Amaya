  # check that sources of Mesa exists
  if test ! -f ../../Mesa/Makefile ; then
    GL_BUILDDIR=""
    GL_INCLUDES="-I/usr/include"
    GL_LIBRARIES="-Wl,-rpath,usr/lib -L/usr/lib -lGL -lGLU"
    echo "---> Use system Mesa library"
