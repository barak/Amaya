	                  'ListStyleImage' ':' ListStyleImage /
	                  'ListStylePosition' ':' ListStylePosition /
			  'Page' '(' BoxName ')' /
			  [ 'Included' ] 'Column' '(' BoxNameList ')' /
			  'Copy' '(' BoxTypeCopied ')' /
			  'Depth' ':' NbInherit /
	                  'Direction' ':' DirInherit /
	                  'UnicodeBidi' ':' BidiInherit .
   54	Rule4 =		  'LineStyle' ':' LineStyleInherit /
			  'LineWeight' ':' DistInherit /
