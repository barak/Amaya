   83	LineStyleInherit = Link '=' / 'Solid' / 'Dashed' / 'Dotted' .
	
   84	FloatInherit =    Link '=' / 'Left' / 'Right' / 'None' .
	
   85	ClearInherit =    Link '=' / 'None' / 'Left' / 'Right' / 'Both' .
	
