			  'Background' ':' Color /
			  'Foreground' ':' Color /
	                  'Opacity' ':' OpacityInherit /
	                  'FillOpacity' ':' OpacityInherit /
	                  'StrokeOpacity' ':' OpacityInherit /
			  'ShowBox' /
			  'BackgroundPicture' ':' FileName /
			  'PictureMode' ':' PictMode .
   55	Rule5 =           'MarginTop' ':' MarginWidth /
	                  'MarginRight' ':' MarginWidth /
	                  'MarginBottom' ':' MarginWidth /
