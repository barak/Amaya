<?xml version="1.0" encoding="UTF-8"?>
<resource version="2.3.0.1" xmlns="http://www.wxwindows.org/wxxrc">
  <object class="wxDialog" name="OpenDocDlgWX">
    <title>*Open/New document*</title>
    <centered>1</centered>
    <style>wxDEFAULT_DIALOG_STYLE|wxWANTS_CHARS|wxRESIZE_BORDER</style>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText" name="wxID_LABEL_DIR">
                  <label>*Directory*</label>
                </object>
                <flag>wxRIGHT|wxALIGN_CENTRE_VERTICAL</flag>
                <border>4</border>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="wxID_DIR"/>
                <option>1</option>
                <flag>wxEXPAND</flag>
              </object>
              <object class="sizeritem">
                <object class="wxBitmapButton" name="wxID_BUTTON_DIR">
                  <bitmap stock_id="wxART_FOLDER"></bitmap>
                </object>
              </object>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>4</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxComboBox" name="wxID_COMBOBOX">
                  <content/>
                  <size>400,-1</size>
                  <focused>1</focused>
                </object>
                <flag>wxEXPAND</flag>
              </object>
              <object class="spacer">
                <size>10,10</size>
              </object>
              <object class="sizeritem">
                <object class="wxBitmapButton" name="wxID_BUTTON_FILENAME">
                  <bitmap stock_id="wxART_FILE_OPEN"></bitmap>
                </object>
              </object>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>4</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxRadioBox" name="wxID_RADIOBOX">
                  <label></label>
                  <content>
                    <item>*In new tab*</item>
                    <item>*Repleace current one*</item>
                    <item>*In new window*</item>
                  </content>
                  <style>wxRA_SPECIFY_COLS</style>
                </object>
                <flag>wxEXPAND</flag>
              </object>
              <object class="spacer">
                <size>10,0</size>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxStaticText" name="wxID_LABEL_FILENAME">
                      <label>*File name*</label>
                      <size>200,-1</size>
                    </object>
                    <option>1</option>
                    <flag>wxEXPAND</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="wxID_FILENAME"/>
                    <option>1</option>
                    <flag>wxEXPAND</flag>
                  </object>
                  <object class="spacer">
                    <size>0,5</size>
                    <option>1</option>
                    <flag>wxALL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxStaticText" name="wxID_PROFILE_LABEL">
                      <label>*profile*</label>
                      <size>200,-1</size>
                      <style>wxALIGN_LEFT</style>
                    </object>
                    <option>1</option>
                    <flag>wxEXPAND</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxComboBox" name="wxID_PROFILE">
                      <content>
                        <item>XHTML 1.1</item>
                        <item>XHTML Transitional</item>
                        <item>XHTML Strict</item>
                        <item>XHTML Basic</item>
                        <item>HTML Transitional</item>
                      </content>
                      <selection>1</selection>
                      <size>150,-1</size>
                      <style>wxCB_READONLY</style>
                    </object>
                    <option>1</option>
                    <flag>wxTOP|wxEXPAND</flag>
                  </object>
                  <object class="spacer">
                    <size>0,5</size>
                  </object>
                </object>
                <option>1</option>
                <flag>wxEXPAND|wxALIGN_CENTRE_VERTICAL</flag>
              </object>
            </object>
            <flag>wxBOTTOM|wxEXPAND</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxALL|wxEXPAND</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <flag>wxALL|wxALIGN_RIGHT|wxADJUST_MINSIZE</flag>
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_OK">
              <label>*Open*</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_CLEAR">
              <label>*Clear*</label>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_CANCEL">
              <label>*Cancel*</label>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
</resource>